{
    "files.watcherExclude": {
        "**/target": true
    },
    "metals.serverVersion": "latest.release",
    "metals.enableSemanticHighlighting": true,
    "metals.enableStripMarginOnTypeFormatting": true,
    "metals.suggestImports": true,
    "metals.completionCommand": "editor.action.triggerSuggest",
    "metals.formatOnSave": true,
    "metals.importFormat": "ascii",
    "metals.slowTask": "show",
    "metals.statusBar": "on",
    "metals.welcomeScreen": false,
    "metals.enableGlobalExecutableSearch": true,
    "metals.sbtScript": "sbt",
    "metals.scalafmtConfigPath": ".scalafmt.conf",
    "metals.mavenScript": "mvn",
    "metals.millScript": "mill",
    "metals.bloopVersion": "latest.release",
    "metals.bloopSbtAlreadyInstalled": false,
    "metals.serverProperties": [
        "-Dmetals.client=vscode",
        "-Dmetals.http=true"
    ]
}